/* pages/apply_result/apply_result.wxss */
.true-img{
  margin: 200rpx auto 0 auto;
  display: block;
  width: 208rpx;
  height: 170rpx;
}
.text{
  color: #333;
  width: 100%;
  height: 36rpx;
  font-size: 36rpx;
  line-height: 36rpx;
  margin-top: 40rpx;
  text-align: center;
}
.user-info-view{
  position: absolute;
  bottom: 0rpx;
  left: 0rpx;
  width: 100%;
  background: #fff;
  padding: 126rpx 0;
}
.user-info{
  width: 522rpx;
  padding: 29rpx 20rpx;
  margin:0 auto 10rpx auto;
  border: 2rpx #6658A6 solid;
  color: #666;
  font-size: 24rpx;
  line-height: 26rpx;
}
.info-text{
  font-size: 20rpx;
}
.false-img{
  margin: 200rpx auto 0 auto;
  display: block;
  width: 182rpx;
  height: 208rpx;
}