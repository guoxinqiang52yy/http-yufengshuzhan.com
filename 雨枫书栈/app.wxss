/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.top-padding{
  height: 4rpx;
  width: 100%;
  background-color: #F2F2F2;
}
page{
  height: 100%;
}
.homePage {
    background: #FFFFFF;
    width: 100%;
}
.searchBarContainer {
    width: 100%;
    height: 90rpx;
    background:#F2F2F2;
    margin-top: 0px;
    top: 0px;
    overflow: hidden;
}


.searchBar{
  height: 60rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
  display: flex;
  overflow: hidden;
  flex-flow: row;
  justify-content: space-around;
  flex-direction: row;
  align-items: center;
}
